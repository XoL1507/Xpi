        "build:staging": "REACT_APP_DATA=staging react-scripts build",
        "build:prod": "REACT_APP_DATA=prod react-scripts build",
        "eslint:check": "eslint --max-warnings=0 .eslintrc.js \"./src/**/*.{js,jsx,ts,tsx}\"",
