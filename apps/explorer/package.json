        "prismjs": "^1.29.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.39.5",
        "react-hot-toast": "^2.4.0",
        "react-router-dom": "^6.4.3",
        "topojson-client": "^3.1.0",
        "web-vitals": "^3.1.0",
        "zod": "^3.19.1"
