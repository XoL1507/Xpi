    "devDependencies": {
        "@babel/core": "^7.20.2",
        "@storybook/addon-a11y": "7.0.0-alpha.38",
        "@storybook/addon-essentials": "7.0.0-alpha.38",
        "@storybook/addons": "7.0.0-alpha.38",
        "@storybook/react": "7.0.0-alpha.38",
        "@storybook/react-vite": "7.0.0-alpha.38",
