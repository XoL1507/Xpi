        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/node": "^18.11.9",
        "@types/react": "^18.0.25",
        "@types/react-dom": "^18.0.9",
        "@types/topojson-client": "^3.1.1",
        "@vitejs/plugin-react": "^2.2.0",
        "@vitest/ui": "^0.25.2",
        "autoprefixer": "^10.4.13",
        "axios": "^1.1.3",
        "cypress": "^11.1.0",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.3.0",
